        }

        field tag_list type array<string> {
            indexing: summary | index
        }

        field read_sex type array<double> {
            indexing: summray | attribute
        }

        field trans_sex type array<double> {
            indexing: summray | attribute
        }

        field read_age type array<double> {
            indexing: summray | attribute
