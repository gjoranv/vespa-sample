
search user {

    document user {

        field user_id type string {
            indexing: summary | index
        }

        field age type int {
            indexing: attribute
        }

        field sex type int {
            indexing: attribute
        }

        field location type tensor(x{}) {
            indexing: attribute
            attribute: tensor(x{})
        }

        field author_score type tensor(x{}) {
            indexing: attribute
            attribute: tensor(x{})
        }

        field novel_score type tensor(x{}) {
            indexing: attribute
            attribute: tensor(x{})
        }

        field user_category_score tensor(x{}) {
            indexing: attribute
            attribute: tensor(x{})
        }

        field user_video_category_score tensor(x{}) {
            indexing: attribute
            attribute: tensor(x{})
        }

    }

}